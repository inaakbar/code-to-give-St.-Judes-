            <tbody>
            <% var x=1; %> 
            <% formList.forEach(form=>{ %>
              <tr>
              <td><%= x++ %>  </td>     
              <td><%= form.type %>  </td>
              <td><%= form.Approval %></td>
              <td><%= form.Doc %></td>
              <td><%= form.Amount %></td>
              <td><%= form.finalStatus %></td>
            </tr>
          <%  })%>
            
          </tbody>
